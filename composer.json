{
  "name": "smilian/perfs-reporter",
  "description": "Measures the performance of PHP code for Symfony projects. Process measurements, lead times, file location...",
  "keywords": ["performance", "speed", "symfony", "measure"],
  "homepage": "https://github.com/Fred-QS/PerformancesLogger",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Frédéric GEFFRAY",
      "email": "frederic.geffray@smile.fr"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {"Smile\\": "src"}
  },
  "scripts": {
    "post-package-install": [
      "@php -r \"mkdir(__DIR__ . '/Test')\""
    ],
    "post-package-update": [
      "@php -r \"mkdir(__DIR__ . '/Test')\""
    ]
  },
  "autoload-dev": {
    "psr-4": {"Smile\\": "tests"}
  }
}